    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "blazeswap": "git+https://github.com/blazeswap/contracts.git",
